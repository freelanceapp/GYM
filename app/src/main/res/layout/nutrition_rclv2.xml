<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<RelativeLayout
    android:background="@drawable/nutrition_rclv_activity_border2"
    android:padding="5dp"
    android:layout_margin="3dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:layout_centerVertical="true"
        android:tint="@color/lightWhite"
        android:id="@+id/nutrition_activity2_images1"
        android:src="@drawable/nutricion_rckvimage2"
        android:layout_width="34dp"
        android:layout_height="34dp" />
    <TextView
        android:textStyle="bold"
        android:layout_marginLeft="10dp"
        android:layout_centerInParent="true"
        android:textAlignment="center"
        android:text="Milk"
        android:id="@+id/nutrition_activity2_text1"
        android:layout_toRightOf="@+id/nutrition_activity2_images1"
        android:textColor="@color/lightWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:textStyle="bold"
        android:layout_marginRight="10dp"
        android:text="100 g"
        android:layout_toLeftOf="@+id/nutrition_activity2_images2"
        android:id="@+id/nutrition_activity2_tex2"
        android:textColor="@color/lightWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/nav_ic_coach"
        android:id="@+id/nutrition_activity2_images2"
        android:layout_width="34dp"
        android:layout_height="34dp" />
</RelativeLayout>
</RelativeLayout>